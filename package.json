{
  "name": "react-tutorial-es6-webpack",
  "version": "0.0.1",
  "description": "react tutorial with es6 and webpack",
  "scripts": {
    "start": "webpack-dev-server",
    "debug": "node dev-server.js",
    "build": "webpack",
    "lint": "eslint src && stylelint src/*.css",
    "test": "mocha --require jsdom-global/register --require ignore-styles --compilers js:babel-register",
    "coverage": "nyc --extension .jsx --extension .es6 --reporter=lcov --reporter=text npm run test"
  },
  "keywords": [
    "react"
  ],
  "author": "daisuzu",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.14.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "remarkable": "^1.6.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-latest": "^6.14.0",
    "babel-preset-power-assert": "^1.0.0",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.14.0",
    "body-parser": "^1.15.2",
    "css-loader": "^0.24.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.4.0",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.2.1",
    "eslint-plugin-react": "^6.2.0",
    "html-webpack-plugin": "^2.22.0",
    "ignore-styles": "^5.0.1",
    "jsdom": "^9.4.5",
    "jsdom-global": "^2.1.0",
    "mocha": "^3.0.2",
    "nyc": "^8.1.0",
    "postcss-loader": "^0.11.1",
    "postcss-reporter": "^1.4.1",
    "power-assert": "^1.4.1",
    "react-addons-test-utils": "^15.3.1",
    "style-loader": "^0.13.1",
    "stylelint": "^7.2.0",
    "stylelint-config-standard": "^13.0.0",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.1"
  }
}
